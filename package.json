{
	"name": "react-circular-input",
	"version": "0.0.0",
	"description": "React components for easily composing a circular range input ðŸŒ€",
	"author": "petecorreia",
	"license": "MIT",
	"homepage": "https://github.com/petecorreia/react-circular-input#readme",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"sideEffects": false,
	"files": [
		"dist"
	],
	"scripts": {
		"start": "tsc -w",
		"build": "yarn clean && tsc",
		"format": "prettier --config .prettierrc --write \"src/**/*\"",
		"clean": "rm -rf dist",
		"lint": "tslint --project tsconfig.json --config tslint.json 'src/**/*.{ts,tsx}'",
		"size": "bundlesize",
		"docz:dev": "docz dev",
		"docz:build": "docz build"
	},
	"devDependencies": {
		"@types/prop-types": "^15.5.8",
		"@types/react": "^16.7.17",
		"bundlesize": "^0.17.0",
		"docz": "^0.12.17",
		"docz-theme-default": "^0.12.17",
		"husky": "^1.2.1",
		"lint-staged": "^8.1.0",
		"prettier": "^1.15.3",
		"react": "^16.7.0-alpha.2",
		"react-codemirror2": "^5.1.0",
		"react-dom": "^16.7.0-alpha.2",
		"react-spring": "^7.2.0",
		"tslint": "^5.11.0",
		"tslint-config-prettier": "^1.17.0",
		"tslint-react": "^3.6.0",
		"typescript": "^3.2.2"
	},
	"bugs": {
		"url": "https://github.com/petecorreia/react-circular-input/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/petecorreia/react-circular-input.git"
	},
	"keywords": [
		"react",
		"circular",
		"input",
		"radial",
		"gauge",
		"react-circular-input",
		"svg",
		"touch",
		"react-component",
		"styled-components",
		"presentational"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"src/**/*.{js,ts,tsx}": [
			"prettier --config .prettierrc --write",
			"git add"
		]
	},
	"bundlesize": [
		{
			"path": "./dist/*.js",
			"maxSize": "2 kB"
		}
	]
}
